   ],
   "source": [
    "def log(base, value):\n",
    "    result = 0\n",
    "    while value > 1:\n",
    "        value /= base\n",
    "        result += 1\n",
    "    return result\n",
    "\n",
    "base = int(input(\"ENTER BASE VALUE\"))\n",
    "value = int(input(\"ENTER VALUE\"))\n",
    "log_r = log(base, value)\n",
    "print(f\"The logarithm of {value} with base {base} is {log_r}\")"
   ]
  },
